{
  "name": "rn-app",
  "private": true,
  "version": "1.0.0",
  "main": "index.ts",
  "scripts": {
    "start": "expo start",
    "storybook": "STORYBOOK_MODE=true expo start",
    "prebuild": "ENVIRONMENT=$1 expo prebuild --skip-dependency-update react-native,react --no-install --platform $0 && echo \"$1\" > \"$0/ENVIRONMENT.txt\"",
    "pod-install": "pod-install && rm -rf ios/.xcode.env.local",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "storybook:ios": "STORYBOOK_MODE=true expo run:ios",
    "storybook:android": "STORYBOOK_MODE=true expo run:android",
    "storybook-generate": "sb-rn-get-stories -js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --pretty",
    "test": "jest --passWithNoTests",
    "clean": "find . -maxdepth 1 \\( -name '*.ipa' -o -name '*.dSYM.zip' -o -name '*.xcarchive' -o -name '*.apk' -o -name '*.aab' \\) -exec rm -rf {} +"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@babel/runtime": "7.23.9",
    "@react-native-async-storage/async-storage": "^1",
    "@react-native-community/datetimepicker": "^7",
    "@react-native-community/slider": "^4",
    "@rnstudy/ios-colors": "workspace:^",
    "@rnstudy/react-hooks": "workspace:^",
    "@rnstudy/react-native-ios-ui": "workspace:^",
    "@rnstudy/react-native-navigation": "workspace:^",
    "@rnstudy/react-native-ui": "workspace:^",
    "@rnstudy/storybook-rn": "workspace:^",
    "@storybook/addon-ondevice-actions": "8.0.0-alpha.2",
    "@storybook/addon-ondevice-controls": "8.0.0-alpha.2",
    "@storybook/react-native": "8.0.0-alpha.2",
    "expo": "~50.0.6",
    "expo-blur": "~12.9.2",
    "expo-build-properties": "~0.11.1",
    "expo-constants": "~15.4.5",
    "expo-dev-client": "~3.3.8",
    "expo-status-bar": "~1.11.1",
    "expo-system-ui": "~2.9.3",
    "react": "18.2.0",
    "react-native": "0.73.4",
    "react-native-gesture-handler": "~2.14.0",
    "react-native-safe-area-context": "4.8.2",
    "react-native-screens": "3.29.0",
    "react-native-sfsymbols": "^1.2.1",
    "react-native-svg": "^14",
    "sweet-sfsymbols": "^0.5.0"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/config-plugins": "^7.8.4",
    "@rnstudy/eslint-config": "workspace:^",
    "@rnstudy/expo-app-config": "workspace:^",
    "@rnstudy/jest-config": "workspace:^",
    "@rnstudy/metro-config": "workspace:^",
    "@rnstudy/tsconfig": "workspace:^",
    "@testing-library/react-native": "^12.4.3",
    "@types/eslint": "^8",
    "@types/jest": "^29",
    "@types/node": "^20",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8",
    "jest": "^29",
    "jest-expo": "^50.0.2",
    "json5": "^2.2.3",
    "pod-install": "^0.2.0",
    "react-test-renderer": "^18",
    "ts-node": "patch:ts-node@npm%3A11.0.0-beta.1#~/.yarn/patches/ts-node-npm-11.0.0-beta.1-cb10a2a461.patch",
    "typescript": "^5"
  }
}
