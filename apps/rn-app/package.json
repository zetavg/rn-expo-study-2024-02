{
  "name": "rn-app",
  "private": true,
  "version": "1.0.0",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "prebuild": "ENVIRONMENT=$0 expo prebuild --skip-dependency-update react-native,react --no-install --platform $1 && rm -rf ios/.xcode.env.local",
    "pod-install": "pod-install",
    "ios": "expo run:ios",
    "android": "expo run:android",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --pretty",
    "test": "jest"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  },
  "dependencies": {
    "@babel/runtime": "7.23.9",
    "expo": "~50.0.6",
    "expo-dev-client": "~3.3.8",
    "expo-status-bar": "~1.11.1",
    "react": "18.2.0",
    "react-native": "0.73.4"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@rnstudy/eslint-config": "workspace:^",
    "@rnstudy/jest-config": "workspace:^",
    "@rnstudy/tsconfig": "workspace:^",
    "@testing-library/react-native": "^12.4.3",
    "@types/eslint": "^8",
    "@types/jest": "^29",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.7",
    "eslint": "^8",
    "jest": "^29",
    "jest-expo": "^50.0.2",
    "json5": "^2.2.3",
    "pod-install": "^0.2.0",
    "ts-node": "patch:ts-node@npm%3A11.0.0-beta.1#~/.yarn/patches/ts-node-npm-11.0.0-beta.1-cb10a2a461.patch",
    "typescript": "^5"
  }
}
